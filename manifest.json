{
	"manifest_version": 2,
	"name": "DouBao",
	"version": "1.0.0",
	"description": "Tool Packages",
	"icons":  {
		"16": "static/images/robot.png",
		"24": "static/images/robot.png",
		"32": "static/images/robot.png",
		"48": "static/images/robot.png"
	},
	"permissions": [
		"<all_urls>",
		"contextMenus",
		"tabs"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_popup": "./popup/popup.html",
		"default_title": ""
	},
	"content_scripts": [],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
